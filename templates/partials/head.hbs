<!doctype html>
<html class="no-js" lang="en-US">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{ page-title }}</title>
        <meta name="description" content="{{ description }}">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="canonical" href="{{ page-url }}">

        <meta property="og:title" content="{{ page-title }}">
        <meta name="twitter:title" content="{{ page-title }}">
        
        <meta property="og:description" content="{{ description }}{{abstract}}">
        <meta name="twitter:description" content="{{ description }}{{abstract}}">
        
        <meta property="og:image" content="{{ page-image }}">
        <meta name="twitter:image" content="{{ page-image }}">

        {{{ favicons }}}

        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Cormorant:ital,wght@0,400;0,700;1,400;1,700&family=Montserrat:ital,wght@0,300;0,500;1,300;1,500&display=swap" rel="stylesheet">

        <link rel="stylesheet" href="/assets/css/style.css">
        {{#each stylesheets}}
            <link rel="stylesheet" href="/assets/css/{{this}}.css">
        {{/each}}
    </head>
